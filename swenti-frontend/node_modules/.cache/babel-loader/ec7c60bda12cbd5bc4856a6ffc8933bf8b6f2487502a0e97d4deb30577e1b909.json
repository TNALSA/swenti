{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  name: 'ArticleView',\n  props: {\n    articleId: {\n      type: [String, Number],\n      // 타입을 String 또는 Number로 변경\n      required: true\n    }\n  },\n  data() {\n    return {\n      cur_articleId: this.articleId,\n      info: {},\n      // 객체 형태로 초기화\n      formattedDate: '',\n      // String 형태로 초기화\n      image: JSON.parse(this.info.images)\n    };\n  },\n  mounted() {\n    const cur_articleId = this.cur_articleId;\n    const router = useRouter();\n    console.log(\"요청하신 글 번호: \", cur_articleId);\n    axios.get(`http://localhost:8080/lookup/details/${cur_articleId}`).then(response => {\n      console.log(\"응답받은 데이터: \", response.data);\n      this.info = response.data.article;\n      this.formattedDate = this.formatDate(this.info.writed_date);\n      router.push(`/article/${cur_articleId}`);\n    }).catch(error => {\n      console.error(\"[Error]\", error);\n    });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      const year = date.getFullYear();\n      const month = date.getMonth() + 1; // 월은 0부터 시작하므로 +1 필요\n      const day = date.getDate();\n      const hours = date.getHours();\n      const minutes = date.getMinutes();\n      const seconds = date.getSeconds();\n      return `${year}년 ${month}월 ${day}일 ${hours}시 ${minutes}분 ${seconds}초`;\n    }\n  }\n};","map":{"version":3,"names":["axios","useRouter","name","props","articleId","type","String","Number","required","data","cur_articleId","info","formattedDate","image","JSON","parse","images","mounted","router","console","log","get","then","response","article","formatDate","writed_date","push","catch","error","methods","dateString","date","Date","year","getFullYear","month","getMonth","day","getDate","hours","getHours","minutes","getMinutes","seconds","getSeconds"],"sources":["C:\\Users\\msa18\\swenti\\swenti-frontend\\src\\views\\ArticleView.vue"],"sourcesContent":["<template>\r\n  <div class=\"article-container\">\r\n    <section class=\"article-header\">\r\n      <div class=\"article-title\">\r\n        <h1>{{ info.title }}</h1>\r\n      </div>\r\n      <div class=\"article-meta\">\r\n        <small>{{ info.writer }}</small>\r\n        <small>{{ formattedDate }}</small>\r\n      </div>\r\n    </section>\r\n    <section class=\"article-content\">\r\n      <img v-if=\"image['image1']\" :src=\"image['image1']\" alt=\"Article Image\" height=\"225\" width=\"100%\"/>\r\n      <p>{{ info.details }}</p>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { useRouter } from \"vue-router\";\r\n\r\nexport default {\r\n  name: 'ArticleView',\r\n  props: {\r\n    articleId: {\r\n      type: [String, Number], // 타입을 String 또는 Number로 변경\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      cur_articleId: this.articleId,\r\n      info: {}, // 객체 형태로 초기화\r\n      formattedDate: '', // String 형태로 초기화\r\n      image: JSON.parse(this.info.images)\r\n    };\r\n  },\r\n  mounted() {\r\n    const cur_articleId = this.cur_articleId;\r\n    const router = useRouter();\r\n    console.log(\"요청하신 글 번호: \", cur_articleId);\r\n\r\n    axios.get(`http://localhost:8080/lookup/details/${cur_articleId}`).then(response => {\r\n      console.log(\"응답받은 데이터: \", response.data);\r\n      this.info = response.data.article;\r\n      this.formattedDate = this.formatDate(this.info.writed_date);\r\n      router.push(`/article/${cur_articleId}`);\r\n    }).catch(error => {\r\n      console.error(\"[Error]\", error);\r\n    });\r\n  },\r\n  methods: {\r\n    formatDate(dateString) {\r\n      const date = new Date(dateString);\r\n      const year = date.getFullYear();\r\n      const month = date.getMonth() + 1; // 월은 0부터 시작하므로 +1 필요\r\n      const day = date.getDate();\r\n      const hours = date.getHours();\r\n      const minutes = date.getMinutes();\r\n      const seconds = date.getSeconds();\r\n\r\n      return `${year}년 ${month}월 ${day}일 ${hours}시 ${minutes}분 ${seconds}초`;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.article-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.article-header {\r\n  border-bottom: 1px solid #ddd;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.article-title h1 {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.article-meta small {\r\n  display: block;\r\n  color: #888;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.article-content p {\r\n  font-size: 16px;\r\n  line-height: 1.6;\r\n}\r\n\r\n.article-content {\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAE,CAACC,MAAM,EAAEC,MAAM,CAAC;MAAE;MACxBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,IAAI,CAACN,SAAS;MAC7BO,IAAI,EAAE,CAAC,CAAC;MAAE;MACVC,aAAa,EAAE,EAAE;MAAE;MACnBC,KAAK,EAAEC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACJ,IAAI,CAACK,MAAM;IACpC,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMP,aAAY,GAAI,IAAI,CAACA,aAAa;IACxC,MAAMQ,MAAK,GAAIjB,SAAS,CAAC,CAAC;IAC1BkB,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEV,aAAa,CAAC;IAEzCV,KAAK,CAACqB,GAAG,CAAC,wCAAwCX,aAAa,EAAE,CAAC,CAACY,IAAI,CAACC,QAAO,IAAK;MAClFJ,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEG,QAAQ,CAACd,IAAI,CAAC;MACxC,IAAI,CAACE,IAAG,GAAIY,QAAQ,CAACd,IAAI,CAACe,OAAO;MACjC,IAAI,CAACZ,aAAY,GAAI,IAAI,CAACa,UAAU,CAAC,IAAI,CAACd,IAAI,CAACe,WAAW,CAAC;MAC3DR,MAAM,CAACS,IAAI,CAAC,YAAYjB,aAAa,EAAE,CAAC;IAC1C,CAAC,CAAC,CAACkB,KAAK,CAACC,KAAI,IAAK;MAChBV,OAAO,CAACU,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;IACjC,CAAC,CAAC;EACJ,CAAC;EACDC,OAAO,EAAE;IACPL,UAAUA,CAACM,UAAU,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,MAAMG,IAAG,GAAIF,IAAI,CAACG,WAAW,CAAC,CAAC;MAC/B,MAAMC,KAAI,GAAIJ,IAAI,CAACK,QAAQ,CAAC,IAAI,CAAC,EAAE;MACnC,MAAMC,GA