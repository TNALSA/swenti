 cur_articleId: this.articleId,\r\n      info: {}\r\n    };\r\n  },\r\n  mounted() {\r\n    const cur_articleId = this.cur_articleId;\r\n    const router = useRouter();\r\n    console.log(\"요청하신 글 번호: \", cur_articleId);\r\n\r\n    axios.get(`http://localhost:8080/lookup/details/${cur_articleId}`).then(response => {\r\n      console.log(\"응답받은 데이터: \", response.data);\r\n      this.info = response.data.article;\r\n      router.push(`/article/${cur_articleId}`);\r\n    }).catch(error => {\r\n      console.error(\"[Error]\", error);\r\n    });\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.article-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.article-header {\r\n  border-bottom: 1px solid #ddd;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.article-title h1 {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.article-meta small {\r\n  display: block;\r\n  color: #888;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.article-content p {\r\n  font-size: 16px;\r\n  line-height: 1.6;\r\n}\r\n\r\n.article-content {\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAE,CAACC,MAAM,EAAEC,MAAM,CAAC;MAAE;MACxBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,IAAI,CAACN,SAAS;MAC7BO,IAAI,EAAE,CAAC;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMF,aAAY,GAAI,IAAI,CAACA,aAAa;IACxC,MAAMG,MAAK,GAAIZ,SAAS,CAAC,CAAC;IAC1Ba,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEL,aAAa,CAAC;IAEzCV,KAAK,CAACgB,GAAG,CAAC,wCAAwCN,aAAa,EAAE,CAAC,CAACO,IAAI,CAACC,QAAO,IAAK;MAClFJ,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEG,QAAQ,CAACT,IAAI,CAAC;MACxC,IAAI,CAACE,IAAG,GAAIO,QAAQ,CAACT,IAAI,CAACU,OAAO;MACjCN,MAAM,CAACO,IAAI,CAAC,YAAYV,aAAa,EAAE,CAAC;IAC1C,CAAC,CAAC,CAACW,KAAK,CAACC,KAAI,IAAK;MAChBR,OAAO,CAACQ,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;IACjC,CAAC,CAAC;EACJ;AACF","igno